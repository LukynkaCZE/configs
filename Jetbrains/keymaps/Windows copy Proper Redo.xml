<keymap version="1" name="Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorSelectWord" />
  <action id="GuiDesigner.QuickJavadoc">
    <mouse-shortcut keystroke="alt button2" />
  </action>
  <action id="HideActiveWindow">
    <keyboard-shortcut first-keystroke="shift escape" />
    <keyboard-shortcut first-keystroke="shift w" />
    <keyboard-shortcut first-keystroke="escape" />
  </action>
  <action id="QuickJavaDoc">
    <mouse-shortcut keystroke="alt button2" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="ctrl q" />
  </action>
  <action id="SmartSelect" />
  <action id="UsageFiltering.WriteAccess" />
  <action id="Vcs.Log.ShowTooltip">
    <mouse-shortcut keystroke="alt button2" />
  </action>
</keymap>